<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alemariey.holdempoker" >

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.dbn.myapplication.JeuxActivity"
            android:label="@string/title_activity_jeux"
            android:theme="@style/Theme.AppCompat.Light" >
        </activity>
        <activity
            android:name="com.example.dbn.myapplication.AddActivity"
            android:label="@string/title_activity_add"
            android:theme="@style/Theme.AppCompat.Light" >
        </activity>
        <activity
            android:name="com.example.dbn.myapplication.AddJetonsActivity"
            android:label="@string/title_activity_add_jetons"
            android:theme="@style/Theme.AppCompat.Light" >
        </activity>
    </application>

</manifest>
